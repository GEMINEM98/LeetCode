class Solution {
public:
    int firstUniqChar(string s) 
    {
        size_t count[256]={0};
        for(size_t i=0;i<s.size();++i)
        {
            count[s[i]]++;
        }
        for(size_t i=0;i<s.size();++i)
        {
            if(count[s[i]]==1)
                return i;
        }
        return -1;
    }
};
